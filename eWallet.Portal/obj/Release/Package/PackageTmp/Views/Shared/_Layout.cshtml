    <script src="/Scripts/nprogress.js"></script>
    <script>
        function loadPage(href) {
            NProgress.start();
            $("#main_content").html('');
            $("#main_content").load(href, function (response, status, xhr) {
                NProgress.done();
                if (status == "error") {
                    var msg = "Sorry but there was an error: ";
                    $("#error").html(msg + xhr.status + " " + xhr.statusText);
                }
            });
        }
    </script>
    
