
@{
    ViewBag.Title = "Rút tiền từ ví";
}
<div class="row">
    <div class="col-md-3 ">
        <ul class="bg nav nav-pills nav-stacked nav-pills-stacked-example">
            <li class="active"><a href="#" class="nav_link" data-href='@Url.Action("CashOutBank", "Account", null, null)'>Yêu cầu rút tiền</a></li>
            <li><a href="#" class="nav_link" data-href='@Url.Action("CashOutBankAccounts","Account",null,null)'>Danh sách tài khoản nhận tiền</a></li>
            <li><a href="#" class="nav_link" data-href='@Url.Action("List","Transaction",null,null)'>Tra soát giao dịch Rút tiền</a></li>
        </ul>
    </div>
    <div class="col-md-9 bg" id="main_content">
        @*@Html.Action("List", "Transaction", null);*@
    </div>
    <div id="error"></div>
</div>
<script>
    $('.nav_link').click(function () {
        $('.nav_link').parent().removeClass("active");
        $(this).parent().addClass("active");
        loadPage($(this).data("href"));
    });
</script>
