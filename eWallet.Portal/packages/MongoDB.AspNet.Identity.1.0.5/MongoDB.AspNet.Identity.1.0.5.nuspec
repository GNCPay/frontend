    <id>MongoDB.AspNet.Identity</id>
    <version>1.0.5</version>
    <title>MongoDB.AspNet.Identity</title>
    <authors>Jonathan Sheely</authors>
    <owners>Jonathan Sheely</owners>
    <licenseUrl>https://raw.github.com/InspectorIT/MongoDB.AspNet.Identity/master/LICENSE</licenseUrl>
