    <script>
        $('ul.slimmenu').slimmenu(
        {
            resizeWidth: '980',
            collapserTitle: '',
            easingEffect: 'easeInOutQuint',
            animSpeed: 'medium',
            indentChildren: true,
            childrenIndenter: ''
        });

        function load_menu_icon() {
            var menu_items = $(".slimmenu a[icon]");
            $(".slimmenu a[icon] i").remove();
            menu_items.each(function (index) {
                var menu_class = $(this).attr("icon");
                $(this).html("<i class='" + menu_class + "'></i>" + $(this).html());
            });
        }
        $(window).resize(function () {
            load_menu_icon();
        });
        load_menu_icon();

        $(document).ready(function () {
            $("#block_page ul").addClass("pagination").addClass("pull-right");
